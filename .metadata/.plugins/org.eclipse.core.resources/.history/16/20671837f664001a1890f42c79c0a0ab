package category;

import org.junit.Test;
import org.junit.experimental.categories.Category;

public class FunctionTest1 
{
	 	@Category(PerformancesTests.class)
	    @Test
	    public void test_a_1() 
	 	{
	        assertThat(1 == 1, is(true));
	    }

	    @Test
	    public void test_a_2() 
	    {
	        assertThat(1 == 1, is(true));
	    }


}



