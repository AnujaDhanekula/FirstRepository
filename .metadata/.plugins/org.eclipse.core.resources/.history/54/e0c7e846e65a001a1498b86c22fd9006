package programs;

import java.util.Scanner;

public class Program5 
{

	public static boolean validateExp(String yop,String exp)
	{
	int cyr=2015;
	int s1=Integer.parseInt(yop);
	int s2=Integer.parseInt(exp);
	
	if((cyr-s1)>=s2)
	return  true;
	else
	return false;
	}
	
	public static void main(String[] args) 
	{
	Scanner sc=new Scanner(System.in);
	System.out.println("Enter employee year of passsing");
	String yop=sc.next();
	System.out.println("Enter Employee Experience");
	String exp=sc.next();
	
	Program5 v=new Program5();
	
	boolean result=v.validateExp(yop,exp);
	System.out.println(result);
	}

	

}


